    "UserWildeisenRecipe": {
        "content_type": "Rezept",
        "type": "entry",
        "fields": {
            "duration": "total_time"
        },
        "links": {
            "many_through": [
                {
                    "relation_to": "UserWildeisenAlergicInfo",
                    "primary_id": "recipe_id",
                    "foreign_id": "alergic_info_id",
                    "through": "UserWildeisenRecipeToAlergicInfo"
                },
                {
                    "relation_to": "UserWildeisenImage",
                    "primary_id": "recipe_id",
                    "foreign_id": "image_id",
                    "through": "UserWildeisenRecipeToImage"
                },
                {
                    "relation_to": "UserWildeisenIngredient",
                    "primary_id": "recipe_id",
                    "foreign_id": "ingredient_id",
                    "through": "UserWildeisenRecipeToIngredient"
                },
//                {
//                    "relation_to": "UserWildeisenPreparationForm",  obadac
//                    "primary_id": "recipe_id",
//                    "foreign_id": "preparation_form_id",
//                    "through": "UserWildeisenRecipeToPreparationForm"
//                },
                {
                    "relation_to": "UserWildeisenBake",
                    "primary_id": "recipe_id",
                    "foreign_id": "bake_id",
                    "through": "UserWildeisenRecipeToPreparationForm"
                },
                {
                    "relation_to": "UserWildeisenOccasion",
                    "primary_id": "recipe_id",
                    "foreign_id": "occasion_id",
                    "through": "UserWildeisenRecipeToOccasion"
                },
                {
                    "relation_to": "UserWildeisenPreparationWay",
                    "primary_id": "recipe_id",
                    "foreign_id": "preparation_way_id",
                    "through": "UserWildeisenRecipeToPreparationWay"
                },
                {
                    "relation_to": "UserWildeisenNutrientContent",
                    "primary_id": "recipe_id",
                    "foreign_id": "nutrient_content_id",
                    "through": "UserWildeisenRecipeToNutrientContent"
                },
                {
                    "relation_to": "UserWildeisenCuisine",
                    "primary_id": "recipe_id",
                    "foreign_id": "cuisine_id",
                    "through": "UserWildeisenRecipeToCuisine"
                },
                {
                    "relation_to": "UserWildeisenIssue",
                    "primary_id": "recipe_id",
                    "foreign_id": "issue_id",
                    "through": "UserWildeisenRecipeToIssue"
                },
                {
                    "relation_to": "UserWildeisenRecipeCategory",
                    "primary_id": "recipe_id",
                    "foreign_id": "recipe_category_id",
                    "through": "UserWildeisenRecipeToRecipeCategory"
                }
            ],
            "many": [
                {
                    "primary_id": "recipe_id",
                    "relation_to": "UserWildeisenRelatedRecipe"
                }
            ]
        }
    },
